<!--==========ZASADY WYPEŁNIANIA PLIKU CONFIG==========-->
<!--1. Żadne pole nie może być puste, chyba że jest nad polem napisane inaczej-->
<!--2. Kolumny w excelu liczone są od 0. Kolumna A = 0, B = 1, itd.-->
<!--3. Cała specyfikacja musi być podana w jednej zakładce (oprócz wersji BIOS, która jest
		obsługiwana osobno). Specyfikacja jest wyszukiwana w zakładce podanej w databaseTableName
		a wersja BIOS w zakładce podanej w biosTableName.-->
<!--4. Kolory można podawać tylko z predefiniowanej listy podanej na końcu dokumentu-->




  <!--=====Nazwa pliku bazy danych=====-->
  <!--UWAGA! Koniecznie ze znakiem '\' na początku.-->
  <filename>\NoteBookiRef_v3.xlsm</filename>
  
  <!--=====Ścieżka dostępu do pliku=====-->
  <!--Jeżeli pusta - to domyślnie wybiera katalog odplenia programu-->
  <filepath></filepath>
  
  <!--=====Nazwa zakładki z modelami=====-->
  <databaseTableName>MD</databaseTableName>
  
  <!--=====Nazwa zakładki z wersjami BIOS=====-->
  <biosTableName>BIOS</biosTableName>
  
  <!--Wearlevel-->
  <!--Jest traktowany jako stała liczba, nie numer kolumny.-->
  <wearLevel>12</wearLevel>
  
  <!--=====Numery kolumn poszczególnych danych======-->
  <!--Zakładka z modelami-->
  <db_Model>0</db_Model>
  <db_PeaqModel>1</db_PeaqModel>
  <db_Storage>6</db_Storage>
  <db_Ram>7</db_Ram>
  <db_Cpu>8</db_Cpu>
  <db_OS>10</db_OS>
  <db_SWM>11</db_SWM>
  <db_CaseModel>14</db_CaseModel>
  <db_MainboardVendor>16</db_MainboardVendor>
  <db_MainboardModel>15</db_MainboardModel>
  <db_ShippingMode>17</db_ShippingMode>
  <db_Tip>13</db_Tip>
  <!--Zakładka z wersjami BIOS-->
  <bios_CaseModel>0</bios_CaseModel>
  <bios_MainboardModel>2</bios_MainboardModel>
  <bios_Bios>3</bios_Bios>
  <bios_EC>4</bios_EC>
  <bios_BuildDate>5</bios_BuildDate>
  
  <!--=====Ustawienia kolorów=====-->
  <!--Poprawne dane-->
  <passColor>Green</passColor>
  <!--Niepoprawne dane-->
  <failColor>Red</failColor>
  <!--Dane na które trzeba zwrócić uwagę-->
  <warningColor>Yellow</warningColor>
  <!--Kolor linii w tabelach-->
  <separatorColor>DarkGoldenrod</separatorColor>
  <!--Nagłówek tabeli specyfikacji-->
  <headerBackgroundColor>Blue</headerBackgroundColor>
  <headerForegroundColor>White</headerForegroundColor>
  <!--Kolory opisów w tabeli specyfikacji-->
  <minorInformationColor>White</minorInformationColor>
  <majorInformationColor>Gray</majorInformationColor>
  <!--Domyślne kolory-->
  <defaultBackgroundColor>Black</defaultBackgroundColor>
  <defaultForegroundColor>White</defaultForegroundColor>
  
